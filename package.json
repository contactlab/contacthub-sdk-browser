{
  "name": "contacthub",
  "version": "0.4.1",
  "description": "Contacthub Tracking Script",
  "main": "src/contacthub.js",
  "scripts": {
    "test": "karma start --single-run",
    "test-watch": "karma start",
    "test-bs": "karma start --single-run --browsers bs_mac_firefox,bs_win7_ie10,bs_android_galaxys5,bs_ios_iphone6",
    "build": "npm run lint && npm run flow && npm run test && npm run webpack && npm run uglify",
    "lint": "eslint src && eslint test",
    "flow": "flow",
    "example": "opn 'http://127.0.0.1.xip.io:8080/example/?workspaceId=WORKSPACE_ID&nodeId=NODE_ID&token=TOKEN' && http-server",
    "webpack": "webpack",
    "uglify": "uglifyjs ./dist/contacthub.js --support-ie8 -c -m -o ./dist/contacthub.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buildo/contacthub.git"
  },
  "keywords": [
    "contactlab",
    "contacthub",
    "tracking"
  ],
  "author": "",
  "license": "",
  "bugs": {
    "url": "https://github.com/buildo/contacthub/issues"
  },
  "homepage": "https://github.com/buildo/contacthub#readme",
