{
  "name": "@contactlab/sdk-browser",
  "version": "2.1.0",
  "description": "Contactlab Browser SDK",
  "author": "Contactlab",
  "license": "Apache-2.0",
  "repository": "contactlab/contacthub-sdk-browser",
  "homepage": "https://contactlab.github.io/contacthub-sdk-browser/",
  "bugs": "https://github.com/contactlab/contacthub-sdk-browser/issues",
  "keywords": [
    "contactlab",
    "contacthub",
    "sdk",
    "tracking"
  ],
  "engines": {
    "node": ">= 16.0",
    "npm": ">= 8.0"
  },
  "main": "dist/sdk.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint '{src,test,lib}/**' --ext .js,.jsx,.ts,.tsx",
    "check": "tsc",
    "karma": "karma start",
    "pretest": "npm run check && npm run lint",
    "test": "jest && npm run karma",
    "test:bs": "jest && BS=true npm run karma",
    "prebuild": "npm run test",
    "build": "webpack",
    "prepublishOnly": "npm run build",
    "example": "open-cli 'http://127.0.0.1:8080/example/?workspaceId=WORKSPACE_ID&nodeId=NODE_ID&token=TOKEN' && http-server",
    "docs": "docs-ts",
    "docs:update": "npm run docs && git add docs",
    "postdocs": "ts-node scripts/docs"
  },
  "dependencies": {
    "cross-fetch": "3.1.4",
    "es6-promise": "4.2.8",
    "fp-ts": "2.11.7",
    "js-cookie": "3.0.1",
    "jssha": "3.2.0",
    "tslib": "2.3.1",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@types/chai": "4.3.0",
    "@types/jest": "27.4.0",
    "@types/js-cookie": "3.0.1",
    "@types/karma": "6.3.2",
    "@types/mocha": "9.0.0",
    "@types/node": "16.11.19",
    "@types/sinon": "10.0.8",
    "@types/terser-webpack-plugin": "5.2.0",
    "@types/uuid": "8.3.4",
    "chai": "4.3.4",
    "docs-ts": "0.6.10",
    "eslint": "8.7.0",
    "eslint-config-contactlab": "8.0.0",
    "eslint-config-prettier": "8.3.0",
    "fetch-mock": "9.11.0",
    "http-server": "14.1.0",
    "husky": "7.0.4",
    "jest": "27.4.7",
    "karma": "6.3.11",
    "karma-browserstack-launcher": "1.6.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-mocha": "2.0.1",
    "karma-webpack": "5.0.0",
    "mocha": "9.1.4",
    "open-cli": "7.0.1",
    "prettier": "2.5.1",
    "pretty-quick": "3.1.3",
    "puppeteer": "13.0.1",
    "sinon": "12.0.1",
    "ts-jest": "27.1.3",
    "ts-loader": "9.2.6",
    "ts-node": "10.4.0",
    "typescript": "4.5.4",
    "webpack": "5.66.0",
    "webpack-cli": "4.9.1",
    "webpack-merge": "5.8.0"
  }
}
