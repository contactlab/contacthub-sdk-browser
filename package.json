    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "eslint": "^3.5.0",
    "eslint-config-buildo": "^0.1.0",
    "eslint-plugin-flowtype": "^2.16.1",
    "flow-bin": "^0.32.0",
    "http-server": "^0.9.0",
    "karma": "^1.1.2",
    "karma-browserstack-launcher": "^1.1.1",
