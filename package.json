{
  "name": "@contactlab/sdk-browser",
  "version": "2.0.0",
  "description": "Contactlab Browser SDK",
  "author": "Contactlab",
  "license": "Apache-2.0",
  "repository": "contactlab/contacthub-sdk-browser",
  "homepage": "https://contactlab.github.io/contacthub-sdk-browser/",
  "bugs": "https://github.com/contactlab/contacthub-sdk-browser/issues",
  "keywords": [
    "contactlab",
    "contacthub",
    "sdk",
    "tracking"
  ],
  "engines": {
    "node": ">= 14.0",
    "npm": ">= 6.13"
  },
  "main": "dist/sdk.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint '{src,test,lib}/**' --ext .js,.jsx,.ts,.tsx",
    "check": "tsc",
    "karma": "karma start",
    "pretest": "npm run check && npm run lint",
    "test": "jest && npm run karma",
    "test:bs": "jest && BS=true npm run karma",
    "prebuild": "npm run test",
    "build": "webpack",
    "prepublishOnly": "npm run build",
    "example": "open-cli 'http://127.0.0.1:8080/example/?workspaceId=WORKSPACE_ID&nodeId=NODE_ID&token=TOKEN' && http-server",
    "docs": "docs-ts",
    "docs:update": "npm run docs && git add docs",
    "postdocs": "ts-node scripts/docs"
  },
  "dependencies": {
    "cross-fetch": "3.1.4",
    "es6-promise": "4.2.8",
    "fp-ts": "2.11.3",
    "js-cookie": "2.2.1",
    "jssha": "3.2.0",
    "tslib": "2.3.1",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@types/chai": "4.2.21",
    "@types/jest": "27.0.1",
    "@types/js-cookie": "2.2.7",
    "@types/karma": "6.3.1",
    "@types/mocha": "9.0.0",
    "@types/node": "14.17.17",
    "@types/sinon": "10.0.2",
    "@types/terser-webpack-plugin": "5.0.4",
    "@types/uuid": "8.3.1",
    "chai": "4.3.4",
    "docs-ts": "0.6.10",
    "eslint": "7.32.0",
    "eslint-config-contactlab": "7.0.0",
    "eslint-config-prettier": "8.3.0",
    "fetch-mock": "9.11.0",
    "http-server": "13.0.1",
    "husky": "7.0.2",
    "jest": "27.2.1",
    "karma": "6.3.4",
    "karma-browserstack-launcher": "1.6.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-mocha": "2.0.1",
    "karma-webpack": "5.0.0",
    "mocha": "9.1.1",
    "open-cli": "7.0.1",
    "prettier": "2.4.1",
    "pretty-quick": "3.1.1",
    "puppeteer": "10.2.0",
    "sinon": "11.1.2",
    "ts-jest": "27.0.5",
    "ts-loader": "9.2.5",
    "ts-node": "10.2.1",
    "typescript": "4.3.5",
    "webpack": "5.53.0",
    "webpack-cli": "4.8.0",
    "webpack-merge": "5.8.0"
  }
}
