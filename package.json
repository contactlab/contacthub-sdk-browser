{
  "name": "@contactlab/sdk-browser",
  "version": "1.4.0",
  "description": "Contactlab Browser SDK",
  "author": "Contactlab",
  "license": "Apache-2.0",
  "repository": "contactlab/contacthub-sdk-browser",
  "homepage": "https://contactlab.github.io/contacthub-sdk-browser/",
  "bugs": "https://github.com/contactlab/contacthub-sdk-browser/issues",
  "keywords": [
    "contactlab",
    "contacthub",
    "sdk",
    "tracking"
  ],
  "engines": {
    "node": ">= 14.0",
    "npm": ">= 6.13"
  },
  "main": "dist/sdk.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint '{src,test,lib}/**' --ext .js,.jsx,.ts,.tsx",
    "prekarma": "webpack",
    "karma": "karma start",
    "pretest": "npm run lint",
    "test": "npm run karma",
    "test:bs": "BS=true npm run karma",
    "uglify": "uglifyjs ./dist/sdk.js --support-ie8 -c -m -o ./dist/sdk.min.js",
    "prebuild": "npm run test",
    "build": "webpack",
    "example": "open-cli 'http://127.0.0.1:8080/example/?workspaceId=WORKSPACE_ID&nodeId=NODE_ID&token=TOKEN' && http-server"
  },
  "dependencies": {
    "es6-promise": "4.2.4",
    "js-cookie": "2.2.0",
    "jssha": "2.3.1",
    "uuid": "3.2.1",
    "xr": "github:dhinus/xr"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-plugin-transform-es3-member-expression-literals": "6.22.0",
    "babel-plugin-transform-es3-property-literals": "6.22.0",
    "babel-plugin-transform-object-assign": "6.22.0",
    "babel-preset-env": "1.7.0",
    "chai": "4.3.4",
    "eslint": "7.26.0",
    "eslint-config-contactlab": "6.2.1",
    "eslint-config-prettier": "8.3.0",
    "http-server": "0.12.3",
    "husky": "6.0.0",
    "karma": "6.3.2",
    "karma-browserstack-launcher": "1.6.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-mocha": "2.0.1",
    "karma-webpack": "5.0.0",
    "mocha": "8.4.0",
    "open-cli": "6.0.1",
    "prettier": "2.3.0",
    "pretty-quick": "3.1.0",
    "puppeteer": "9.1.1",
    "sinon": "10.0.0",
    "uglify-js": "3.3.23",
    "webpack": "5.37.1",
    "webpack-cli": "4.7.0",
    "webpack-merge": "5.7.3"
  }
}
